        "title": "proxy Adapter",
        "desc": "This adapter allows to reach other HTTP servers (like WEB CAM) in the same web server",
        "authors": [
            "bluefox <dogafox@gmail.com>"
        ],
        "license": "MIT",
        "platform": "Javascript/Node.js",
        "mode": "extension",
        "loglevel": "info",
        "icon": "proxy.png",
        "webExtension": "lib/proxy.js",
        "readme": "https://github.com/ioBroker/ioBroker.proxy/blob/master/README.md",
        "keywords": [
            "web",
            "proxy",
            "communication"
        ],
        "enabled": true,
        "extIcon": "https://raw.githubusercontent.com/ioBroker/ioBroker.proxy/master/admin/proxy.png",
        "type": "network",
        "dependencies": [
            {
                "js-controller": ">=0.15.0"
            }
        ]
    },
    "native": {
        "route": "",
        "webInstance": "",
        "errorTimeout": 10000,
        "rules": [
            {
                "regex": "sonos/",
                "url": "http://localhost:8083"
            },
            {
                "regex": "api/",
                "url": "http://localhost:8084"
            },
            {
                "regex": "node-red/",
                "url": "http://localhost:1880"
            }
        ]
    }
}